{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "start:server": "npx json-server public/data/user.json --host localhost --port 8080",
    "preview": "vite preview",
    "prepare": "husky",
    "type-check": "vue-tsc --noEmit",
    "type-check:src": "vue-tsc --noEmit --project tsconfig.app.json",
    "type-check:staged": "vue-tsc --noEmit --project tsconfig.app.json",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/vue-table": "^8.21.3",
    "@vueuse/core": "^14.2.0",
    "axios": "^1.13.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "json-server": "^1.0.0-beta.5",
    "lucide-vue-next": "^0.562.0",
    "pinia": "^3.0.4",
    "reka-ui": "^2.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.24",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vue-tsc": "^3.1.4"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "bash -c 'npm run type-check:src'"
    ]
  }
}
